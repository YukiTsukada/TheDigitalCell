// example of for loop
for (i=0; i<10; i++) {
	j = 5*i;
	print(j);
	}
// example of while loop
i = 0;
while (i<=90) {
	print(i);
	i = i + 5;
	}
// example of do-while loop
i = 0;
do {
	print(i);
	i = i + 5;
	} while (i<=90);
// adding complexity to a for loop
for (i=0; i<10; i++) {
	if (i%2==0) {
	    j = 5*i;
		print(j);
		}
	}
